        item.name =  "Palette (+12eyeshadows)";
      }

      if(!cartItem.hide) {
        items.push(item);
      }      
    });

    // Gather all cart data and add to DOM
    data = {
      items: items,
      note: cart.note,
      totalPrice: Shopify.formatMoney(cart.total_price, settings.moneyFormat)
    }

    $cartContainer.append(template(data));

    cartCallback(cart);
    
