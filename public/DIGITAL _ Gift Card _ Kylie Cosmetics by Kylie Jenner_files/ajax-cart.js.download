      freeGiftQtyGreaterTwo(cart);
    } else if (window.freeGiftFromPrice) { console.warn('Cart Callback, Free Gift From PRice');
      freeGiftFromPrice(cart);
    } else if (window.FreeGiftCartProduct) {
      freeGiftCart(cart);
    }

